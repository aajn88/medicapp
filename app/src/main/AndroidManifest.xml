<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.crossover.medicapp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_medicapp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".controllers.LoginActivity"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity
            android:name=".controllers.create_account.CreateAccountActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">
        </activity>
        <activity android:name=".controllers.HomeActivity">
        </activity>
        <activity android:name=".controllers.calendar.EventDetailActivity">
        </activity>
        <activity android:name=".controllers.calendar.CreateEventActivity">
        </activity>
    </application>

</manifest>
