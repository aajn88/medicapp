<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/event_cell_ll"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/cell_bg"
    android:minHeight="80dp"
    android:orientation="vertical"
    android:paddingLeft="3dp"
    android:paddingRight="3dp"
    android:paddingTop="3dp"
    android:transitionName="@string/transition_event">

    <com.crossover.medicapp.custom_views.font.RobotoTextView
        android:id="@+id/tv1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:textSize="10sp"
        app:typeface="robotoRegular"/>

    <LinearLayout
        android:id="@+id/events_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"/>

    <ImageView
        android:id="@+id/ellipsis_iv"
        android:layout_width="20dp"
        android:layout_height="10dp"
        android:layout_marginTop="2dp"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_ellipsis"/>

</LinearLayout>